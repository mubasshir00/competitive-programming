#include<bits/stdc++.h>
using namespace std ;
int main()
{
    string a ;
    string b ;
    vector<int>v;
    cin>>a;
    cin>>b;
    for(int i=0;i<a.length();i++)
        for(int i=0;i<b.length();i++)
    {
        if(a[i]==b[i])
        {
            v.push_back(0);
        }
        if(a[i]!=b[i])
        {
            v.push_back(1);
        }

    }
    for(int i=0;i<a.length();i++)
    {
        cout<<v[i];
    }
    cout<<endl;
    v.clear();

    return 0 ;
}
