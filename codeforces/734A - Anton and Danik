#include<bits/stdc++.h>
using namespace std ;
int main()
{
    vector<char>v;
    int n;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        char x ;
        cin>>x;
        v.push_back(x);
    }
    int ca =0;
    int cb =0 ;
    for(int i=0;i<n;i++)
    {
        if(v[i]=='A')ca++;
        else if(v[i]=='D')cb++;
    }
    if(ca>cb)cout<<"Anton"<<endl;
    else if(ca<cb)cout<<"Danik"<<endl;
    else cout<<"Friendship"<<endl;

    return 0;
}
