#include<bits/stdc++.h>
using namespace std ;
int main()
{
    double  h ;
    int n ;
    cin>>n>>h;
    int sum =0;
    int x[n] ;
    for(int i=0;i<n;i++)
    {
        cin>>x[i];
    }
    for(int i=0;i<n;i++)
    {
        sum = sum + ceil(x[i]/h);

    }

    cout<<sum<<endl;
    return 0 ;
}
