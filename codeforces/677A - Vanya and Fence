#include<bits/stdc++.h>
using namespace std ;
int main()
{
    int h,n;
    int a[10000];
    cin>>n>>h;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int L =0,B=0;
    for(int i=0;i<n;i++)
    {
        if(a[i]<=h)L++;
        if(a[i]>h)B=B+2;
    }
    cout<<L+B<<endl;
    return 0 ;
}
